rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /{allPaths=**} {
      allow read: if request.auth!=null
      allow write: if request.auth!=null && request.resource.size < 10 * 1024 * 1024 && request.resource.contentType.matches('image/.*');
      allow delete: if false
                  
    }
  }
}